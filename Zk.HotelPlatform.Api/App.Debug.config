<?xml version="1.0" encoding="utf-8"?>

<!-- 有关使用 web.config 转换的详细信息，请访问 http://go.microsoft.com/fwlink/?LinkId=125889 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <connectionStrings>
    <add name="HotelPlatform" connectionString="Data Source=192.168.1.252;Initial Catalog=HotelPlatform;Persist Security Info=True;User ID=sa;Password=123456" providerName="System.Data.SqlClient"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)" />
  </connectionStrings>

  <redisconfig WriteServerList="123456@192.168.1.252:6379" ReadServerList="123456@192.168.1.252:6379" MaxWritePoolSize="10000" MaxReadPoolSize="10000" AutoStart="true" LocalCacheTime="180" RecordeLog="false"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)" />
</configuration>